#include<stdio.h>
int fibo(int n)
{
	if (n==0 || n==1)
	{
		return n;
	}
	else
	{
		return fibo(n-1) + fibo(n-2);
	}
}
int main()
{
	int n,i;
	printf("enter the value of n: ");
	scanf("%d",&n);
	printf("the febinacci series: ");
	for(i=0;i<=n;i++)
	{
		printf("%d ",fibo(i));
	}
	printf("\n");
	return 0;
}
